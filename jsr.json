{
  "name": "@signaler/cli",
  "version": "2.1.4",
  "description": "Next-generation web performance monitoring platform with AI-powered insights, enterprise-grade optimizations, and comprehensive CI/CD integration",
  "keywords": [
    "performance",
    "monitoring",
    "web-performance",
    "lighthouse",
    "accessibility",
    "seo",
    "audit",
    "cli",
    "typescript",
    "javascript",
    "ci-cd",
    "automation",
    "testing",
    "optimization",
    "analytics",
    "reporting"
  ],
  "license": "MIT",
  "repository": "https://github.com/signaler/signaler",
  "homepage": "https://signaler.dev",
  "readme": "README.md",
  "exports": {
    ".": "./src/index.ts",
    "./api": "./src/api.ts"
  },
  "publish": {
    "include": [
      "src/",
      "dist/",
      "scripts/postinstall.js",
      "README.md",
      "CHANGELOG.md",
      "LICENSE"
    ],
    "exclude": [
      "dist/**/*.test.*",
      "dist/**/*.spec.*",
      "dist/**/*.map",
      "dist/**/test/",
      "dist/**/tests/",
      "dist/**/__tests__/",
      "dist/**/*.development.*",
      "dist/**/*.dev.*",
      "**/.DS_Store",
      "**/Thumbs.db",
      "**/*.log",
      "**/*.tmp",
      "**/*.temp",
      "**/*.tsbuildinfo",
      "**/node_modules/",
      "**/.git/",
      "**/.github/",
      "**/.kiro/",
      "**/test/",
      "**/tests/",
      "**/__tests__/",
      "**/coverage/",
      "**/.nyc_output/",
      "**/playwright-report/",
      "**/test-results/",
      "**/*.config.ts",
      "**/*.config.js",
      "**/tsconfig*.json",
      "**/.env*",
      "**/pnpm-lock.yaml",
      "**/package-lock.json",
      "**/yarn.lock",
      "**/.npmrc",
      "**/AI-*.md",
      "**/CLEANUP-*.md",
      "**/CONTRIBUTING.md",
      "**/INSTALLATION-GUIDE.md",
      "**/KNOWN-ISSUES.md",
      "**/RELEASE-*.md",
      "**/engine.manifest.json",
      "**/signaler-test.json",
      "**/publish-jsr.*",
      "**/*.bat",
      "**/*.sh"
    ]
  }
}